{
  "kind": "collectionType",
  "collectionName": "persons",
  "info": {
    "singularName": "person",
    "pluralName": "persons",
    "displayName": "Person",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "gndIdentifier": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string",
      "regex": "(Keine GND-Ansetzung für .+)|(1[012]?\\d{7}[0-9X]|[47]\\d{6}-\\d|[1-9]\\d{0,7}-[0-9X]|3\\d{7}[0-9X])",
      "required": false,
      "unique": true
    },
    "rppdId": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string",
      "required": false,
      "unique": true
    },
    "consentGiven": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "boolean",
      "default": true
    },
    "doNotCrawl": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "doNotIndex": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "doNotFeature": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "publication": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.publication"
    },
    "source": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.source",
      "required": false
    },
    "biographicalOrHistoricalInformation": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.biographical-or-historical-information"
    },
    "preferredName": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string",
      "required": true
    },
    "variantName": {
      "displayName": "variantName",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.variant-name"
    },
    "otherName": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.other-name"
    },
    "professionOrOccupation": {
      "displayName": "professionOrOccupation",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.profession-or-occupation",
      "required": false
    },
    "gender": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "enumeration",
      "enum": [
        "männlich",
        "weiblich",
        "divers"
      ]
    },
    "gndSubjectCategory": {
      "displayName": "gndSubjectCategory",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.gnd-subject-category"
    },
    "placeOfDeath": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "customField",
      "options": {
        "source": {
          "RPB-Raumsystematik": true,
          "GND-Geografika": true
        },
        "detached": true
      },
      "customField": "plugin::lookup.lookup"
    },
    "placeOfBirth": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "customField",
      "options": {
        "source": {
          "RPB-Raumsystematik": true,
          "GND-Geografika": true
        },
        "detached": true
      },
      "customField": "plugin::lookup.lookup"
    },
    "placeOfActivity": {
      "displayName": "placeOfActivity",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.place-of-activity"
    },
    "dateOfBirth": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string"
    },
    "dateOfDeath": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string"
    },
    "relatedPerson": {
      "displayName": "relatedPerson",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "person.related-person"
    },
    "depictionId": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string"
    },
    "depiction": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      }
    },
    "periodOfActivity": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string"
    },
    "doDelete": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "internalNote": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "text"
    },
    "dateModified": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "date"
    }
  }
}

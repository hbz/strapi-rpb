    "inCollection": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "enumeration",
      "enum": [
        "nur RPB",
        "RPB und BiblioVino",
        "nur BiblioVino"
      ],
      "default": "nur RPB",
      "required": true
    },
    "extent": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string"

{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Aufsatz",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "rpbId": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string",
      "required": true,
      "unique": true
    },
    "doNotIndex": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "person": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "rpb.person"
    },
    "corporateBody": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "component": "rpb.corporate-body"
    },
    "preferredTitle": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string"
    },
    "title": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "text",
      "required": true
    },
    "otherTitleInformation": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string"
    },
    "parallelTitle": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "versions": {
          "versioned": true

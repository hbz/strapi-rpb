    },
    "publicationDate": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string",
      "required": true
    },
    "publicationYear": {
      "pluginOptions": {
        "versions": {
          "versioned": true
        }
      },
      "type": "string",
      "required": true,
      "regex": "\\d{4}"
